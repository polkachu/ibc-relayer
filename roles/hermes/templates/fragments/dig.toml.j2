[[chains]]
id = 'dig-1'
{% include 'fragments/ports.toml.j2' %}


rpc_timeout = '30s'
account_prefix = 'dig'
key_name = 'dig'
store_prefix = 'ibc'
max_gas = 7000000
max_tx_size = 2097152
gas_price = { price = 0.001, denom = 'udig' }
gas_multiplier = 2
clock_drift ='5s'
trusting_period = '7days'
memo_prefix = '{{ memo }}'

[chains.packet_filter]
policy = 'allow'
list = [
  ['transfer', '*'],
]
