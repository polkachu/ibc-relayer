[[chains]]
id = 'impacthub-3'
{% include 'fragments/ports.toml.j2' %}


rpc_timeout = '30s'
account_prefix = 'ixo'
key_name = 'ixo'
store_prefix = 'ibc'
max_tx_size = 180000
max_gas = 3000000
max_msg_num = 30
gas_price = { price = 0, denom = 'ixo' }
gas_adjustment = 1
clock_drift ='70s'
trusting_period = '14days'
trust_threshold = { numerator = '1', denominator = '3' }
memo_prefix = '{{ memo }}'

[chains.packet_filter]
policy = 'allow'
list = [
  ['transfer', '*'],
]
