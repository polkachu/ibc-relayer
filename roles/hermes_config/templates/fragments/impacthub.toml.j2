[[chains]]
id = 'impacthub-3'
{% set rpc_port = port_prefixes.json[item.key] if port_prefix_url is defined else default_rpc %}
{% set grpc_port = port_prefixes.json[item.key] if port_prefix_url is defined else default_grpc %}
rpc_addr = 'http:/{{ item.value.ip }}:{{ rpc_port }}57'
grpc_addr = 'http:/{{ item.value.ip }}:{{ grpc_port }}90'
websocket_addr = 'ws:/{{ item.value.ip }}:{{ rpc_port }}57/websocket'

rpc_timeout = '30s'
account_prefix = 'ixo'
key_name = 'ixo'
store_prefix = 'ibc'
max_tx_size = 180000
max_gas = 3000000
max_msg_num = 30
gas_price = { price = 0, denom = 'ixo' }
gas_adjustment = 1
clock_drift ='70s'
trusting_period = '10days'
trust_threshold = { numerator = '1', denominator = '3' }
memo_prefix = '{{ memo }}'

[chains.packet_filter]
policy = 'allow'
list = [
  ['transfer', '*'],
  ['wasm.juno1v4887y83d6g28puzvt8cl0f3cdhd3y6y9mpysnsp3k8krdm7l6jqgm0rkn', 'channel-47'], # Osmosis-CW20
]
